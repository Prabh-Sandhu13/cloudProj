<style>
    table, th, td {
        padding: 10px;
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>
<body>
    <h1 class="display-4">Jobs Info</h1><br>
    <table>
        <thread>
            <tr>
                <th>Job Name </th>
                <th>Part ID </th>
                <th>Quantity </th>
                <th>Actions</th>
            </tr>
        </thread>
        <tbody><% jobs735.forEach(function(job735){ %><tr>
            <td><%= job735.id %> </td>
            <td><%= job735.partId %> </td>
            <td><%= job735.qty %> </td>
            <td><form
                class="d-inline"
                action="/editJob/<%=job735.id%>/<%=job735.partId%>"
                method="POST"
                >
                <input type="submit" value="Update" class="btn btn-primary" />
                </form>
            </td>
            </tr><% }) %>
        </tbody>
    </table>
</body><br>
<form action="/">
    <button class="btn btn-light">Home </button>
</form><br>
<form action="/addData">
    <button class="btn btn-light">Add Data </button>
</form>